//write a command to grab the number of members in a fivem server from the API here https://servers-frontend.fivem.net/api/servers/single/lbjmv7 and display it in an embed
const { SlashCommandBuilder } = require('@discordjs/builders');
const { EmbedBuilder } = require('discord.js');
const { request } = require('undici');
module.exports = {
    data: new SlashCommandBuilder()
        .setName('count')
        .setDescription('Get the number of players on the server.'),
    async execute(interaction) {
        const embed = new EmbedBuilder()
            .setTitle("Server Player Count")
            .setColor("Blue")
            .setTimestamp()
            .setFooter({ text: "Requested by " + interaction.user.tag })
            const cat = await request('https://servers-frontend.fivem.net/api/servers/single/lbjmv7')
            .then(res => res.json())
            .then(json => {
                embed.setDescription(`There are currently ${json.data.clients} players on the server.`);
                interaction.reply({ embeds: [embed] });
            })
    },
};